<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:splitMotionEvents="false"
    tools:context=".view.view.activity.CharacterCreatedActivity">

    <TextView
        android:id="@+id/character_created_create_character_text_id"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:background="@drawable/frame_line_03"
        android:gravity="center"
        android:text="@string/character_created_create_character_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/character_created_character_back_button_id"
        android:layout_width="80dp"
        android:layout_height="36dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/frame_line_02"
        android:text="@string/character_created_character_back_print__button"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@android:color/background_light"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/character_created_create_character_text_id" />

    <LinearLayout
        android:id="@+id/character_created_job_name_linearLayout"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:orientation="vertical"
        android:splitMotionEvents="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_create_character_text_id">

        <TextView
            android:id="@+id/character_created_character_name_text_id"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/character_created_character_job_text_id"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textSize="30sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/character_created_character_continuously_character_text_id"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/circle_angle_quadrangle_01"
        android:gravity="center"
        android:text="@string/character_created_character_Continuously_character_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/character_created_character_end_creation_text_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_luck_text_id" />

    <TextView
        android:id="@+id/character_created_character_end_creation_text_id"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/circle_angle_quadrangle_01"
        android:gravity="center"
        android:text="@string/character_created_character_end_creation_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_continuously_character_text_id" />

    <TextView
        android:id="@+id/character_created_hp_print_text_id"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:text="@string/character_created_hp_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_status_print_text_id" />

    <TextView
        android:id="@+id/character_created_mp_print_text_id"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="@string/character_created_mp_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_hp_print_text_id" />

    <TextView
        android:id="@+id/character_created_str_print_text_id"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="@string/character_created_str_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_mp_print_text_id" />

    <TextView
        android:id="@+id/character_created_def_print_text_id"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="@string/character_created_def_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_str_print_text_id" />

    <TextView
        android:id="@+id/character_created_agi_print_text_id"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="@string/character_created_agi_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_def_print_text_id" />

    <TextView
        android:id="@+id/character_created_luck_print_text_id"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:text="@string/character_created_luck_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_agi_print_text_id" />

    <TextView
        android:id="@+id/character_created_character_hp_text_id"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:gravity="right"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/character_created_hp_print_text_id"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_status_print_text_id" />

    <TextView
        android:id="@+id/character_created_character_str_text_id"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:gravity="right"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/character_created_str_print_text_id"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_mp_text_id" />

    <TextView
        android:id="@+id/character_created_character_mp_text_id"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:gravity="right"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/character_created_mp_print_text_id"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_hp_text_id" />

    <TextView
        android:id="@+id/character_created_character_def_text_id"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:gravity="right"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/character_created_def_print_text_id"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_str_text_id" />

    <TextView
        android:id="@+id/character_created_character_agi_text_id"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:gravity="right"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/character_created_agi_print_text_id"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_def_text_id" />

    <TextView
        android:id="@+id/character_created_character_luck_text_id"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:gravity="right"
        android:text=""
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/character_created_luck_print_text_id"
        app:layout_constraintTop_toBottomOf="@+id/character_created_character_agi_text_id" />

    <TextView
        android:id="@+id/character_created_character_status_print_text_id"
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:text="@string/character_created_character_status_print_text"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/character_created_job_name_linearLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>