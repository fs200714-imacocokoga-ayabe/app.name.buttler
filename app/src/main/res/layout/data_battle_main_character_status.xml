<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:splitMotionEvents="false">

    <LinearLayout
        android:id="@+id/data_battle_main_character_status_linearLayout_id"
        android:layout_width="116dp"
        android:layout_height="match_parent"
        android:background="@drawable/frame_line_01"
        android:orientation="vertical"
        android:scaleType="centerCrop"
        android:splitMotionEvents="false"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX=""
        tools:layout_editor_absoluteY="">

        <TextView
            android:id="@+id/data_battle_main_character_status_member_name_text_id"
            android:layout_width="116dp"
            android:layout_height="18dp"
            android:gravity="left"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/data_battle_main_character_status_member_hp_text_id"
            android:layout_width="116dp"
            android:layout_height="17dp"
            android:gravity="left"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/data_battle_main_character_status_member_mp_text_id"
            android:layout_width="116dp"
            android:layout_height="17dp"
            android:gravity="left"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/data_battle_main_character_status_member_status_text_id"
            android:layout_width="116dp"
            android:layout_height="18dp"
            android:gravity="center"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="116dp"
        android:layout_height="70dp"
        android:background="@drawable/frame_line_07"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/data_battle_main_character_status_linearLayout_id"
        app:layout_constraintEnd_toEndOf="@+id/data_battle_main_character_status_linearLayout_id"
        app:layout_constraintStart_toStartOf="@+id/data_battle_main_character_status_linearLayout_id"
        app:layout_constraintTop_toTopOf="@+id/data_battle_main_character_status_linearLayout_id" />


</androidx.constraintlayout.widget.ConstraintLayout>