<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:splitMotionEvents="false">

    <LinearLayout
        android:id="@+id/data_battle_main_character_status_id"
        android:layout_width="131sp"
        android:layout_height="70sp"
        android:background="@drawable/frame_line_01"
        android:orientation="vertical"
        android:splitMotionEvents="false"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX=""
        tools:layout_editor_absoluteY="">

        <TextView
            android:id="@+id/member_name_id"
            android:layout_width="131dp"
            android:layout_height="18sp"
            android:gravity="left"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/member_hp_id"
            android:layout_width="131dp"
            android:layout_height="17sp"
            android:gravity="left"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/member_mp_id"
            android:layout_width="131dp"
            android:layout_height="17sp"
            android:gravity="left"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/member_status_id"
            android:layout_width="131dp"
            android:layout_height="18sp"
            android:gravity="center"
            android:text=""
            android:textAppearance="@style/TextAppearance.AppCompat.Small"
            android:textColor="?android:attr/textColorPrimary"
            android:textSize="15sp" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>